{
  "name": "@jsenv/template-pwa",
  "version": "1.0.0",
  "private": true,
  "description": "Package description",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-template-pwa"
  },
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "dev": "node ./script/dev/dev.mjs",
    "eslint": "node ./node_modules/eslint/bin/eslint.js . --ext=.js,.html",
    "importmap": "node ./script/importmap/importmap.mjs",
    "build": "node ./script/build/build.mjs",
    "build-serve": "node ./script/build/build_serve.mjs",
    "file-size": "node ./script/file_size/file_size.mjs --log",
    "lighthouse": "node ./script/lighthouse/lighthouse.mjs --local",
    "performance": "node ./script/performance/performance.mjs --log --once",
    "test": "node ./script/test/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "start": "npm run build && npm run build-serve",
    "prettier": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "certificate-install": "node ./script/dev/install_certificate.mjs",
    "heroku-postbuild": "npm run importmap && npm run build"
  },
  "dependencies": {
    "@jsenv/pwa": "4.0.1",
    "@jsenv/server": "12.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/eslint-parser": "7.16.5",
    "@jsenv/assert": "2.4.0",
    "@jsenv/babel-preset": "1.1.2",
    "@jsenv/core": "25.0.0-alpha.3",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/file-size-impact": "12.1.1",
    "@jsenv/filesystem": "2.6.0",
    "@jsenv/https-local": "1.0.3",
    "@jsenv/importmap-eslint-resolver": "5.2.2",
    "@jsenv/importmap-node-module": "5.0.0",
    "@jsenv/lighthouse-impact": "2.1.2",
    "@jsenv/performance-impact": "2.2.1",
    "eslint": "7.32.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "playwright": "1.17.1",
    "prettier": "2.5.1"
  }
}
