{
  "name": "@jsenv/template-pwa",
  "version": "1.0.0",
  "private": true,
  "description": "Package description",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-template-pwa"
  },
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./*": "./*"
  },
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node ./scripts/dev.mjs",
    "build:local": "node ./scripts/build.mjs",
    "build:prod": "node ./scripts/build.mjs --prod",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "prettier": "prettier --write .",
    "lighthouse": "node ./scripts/lighthouse.mjs --local --open-report",
    "performance": "node ./scripts/performance.mjs --log",
    "certificate:install": "node ./scripts/install_certificate.mjs",
    "playwright:install": "npx playwright install-deps && npx playwright install"
  },
  "dependencies": {
    "@jsenv/pwa": "6.0.2",
    "@jsenv/service-worker": "1.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.7",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "@jsenv/assert": "2.9.0",
    "@jsenv/core": "37.0.3",
    "@jsenv/eslint-config": "16.4.0",
    "@jsenv/eslint-import-resolver": "8.0.0",
    "@jsenv/filesystem": "4.2.4",
    "@jsenv/https-local": "3.0.7",
    "@jsenv/lighthouse-impact": "4.0.4",
    "@jsenv/performance-impact": "4.1.1",
    "@jsenv/plugin-bundling": "2.3.0",
    "@jsenv/plugin-explorer": "1.0.1",
    "@jsenv/plugin-minification": "1.4.0",
    "@jsenv/plugin-placeholders": "1.1.4",
    "@jsenv/test": "1.4.0",
    "eslint": "8.44.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "open": "9.1.0",
    "playwright": "1.35.1",
    "prettier": "3.0.0"
  }
}
