{
  "name": "@jsenv/template-pwa",
  "version": "1.0.0",
  "private": true,
  "description": "Package description",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-template-pwa"
  },
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./*": "./*"
  },
  "scripts": {
    "dev": "node ./scripts/dev/dev.mjs",
    "test": "node ./scripts/test/test.mjs",
    "preview": "node ./scripts/build/preview.mjs",
    "build": "node ./scripts/build/build.mjs --preview",
    "eslint": "npx eslint . --ext=.js,.html,.mjs",
    "file-size": "node ./scripts/file_size/file_size.mjs --log",
    "lighthouse": "node ./scripts/lighthouse/lighthouse.mjs --local",
    "performance": "node ./scripts/performance/performance.mjs --log --once",
    "test-with-coverage": "npm run test -- --coverage",
    "start": "npm run preview",
    "prettier": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "certificate-install": "node ./scripts/dev/install_certificate.mjs"
  },
  "dependencies": {
    "@jsenv/pwa": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-syntax-import-assertions": "7.16.7",
    "@jsenv/assert": "2.5.4",
    "@jsenv/core": "27.0.0-alpha.36",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/eslint-import-resolver": "0.1.5",
    "@jsenv/file-size-impact": "12.1.12",
    "@jsenv/filesystem": "3.1.0",
    "@jsenv/https-local": "1.0.8",
    "@jsenv/lighthouse-impact": "2.1.11",
    "@jsenv/performance-impact": "2.2.11",
    "eslint": "8.15.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "playwright": "1.21.1",
    "prettier": "2.6.2"
  }
}
